<div id="statistics-body">
<ul class="list-group">
	 <li class="list-group-item disabled">
        <div class="row">
          <div class="col-md-3">url
          </div>
          <div class="col-md-3">browser
          </div>
		  <div class="col-md-3">language
          </div>
          <div class="col-md-3">clicks
          </div>
      </div>
      
<%  @data.each do |entry| %>
  <li class="list-group-item">
          <div class="row">

            <div class="col-md-3"  > <%= link_to entry["longUrl"], retrieve_path( :url => entry["longUrl"]) %></div>
            <div class="col-md-3"  ><%= browerName(entry["userAgent"]) %></div>
            <div class="col-md-3"  ><%= sanitize(entry["lang"]) %></div>
           	<div class="col-md-3"  ><%= entry["count"] %></div>
           	<div class="col-md-3"  ><%= entry["time"] %></div>
          </div>     
      </li>
<% end %>
</ul>
</div>